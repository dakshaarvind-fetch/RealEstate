# Agent identity
AGENT_SEED=replace_with_private_seed
AGENT_NAME=real_estate_agent
AGENT_NETWORK=testnet
AGENT_MAILBOX=true

# Required API key
ANTHROPIC_API_KEY=replace_with_anthropic_key

# Google OAuth client (Desktop app or Web app JSON from Google Cloud)
# Preferred: paste JSON as one line.
GOOGLE_OAUTH_CLIENT_JSON={"installed":{"client_id":"replace.apps.googleusercontent.com","project_id":"replace","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_secret":"replace","redirect_uris":["http://localhost"]}}
# Alternative: point to a JSON file instead.
# GOOGLE_OAUTH_CLIENT_FILE=oauth_client.json

# Optional operational settings
# GOOGLE_SHEET_SHARE_EMAIL=
# GOOGLE_OAUTH_TOKEN_STORE_FILE=google_user_tokens.json
# GOOGLE_OAUTH_DEVICE_STORE_FILE=google_device_flows.json

# Optional for non-mailbox local transport
# AGENT_PORT=8001
# AGENT_ENDPOINT=http://127.0.0.1:8001/submit
